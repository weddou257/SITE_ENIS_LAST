@model IEnumerable<ASP.NETCoreIdentityCustom.Models.Clubs>


<div class="container p-3">
    <div class="row pt-4">
        <div class="col-6">
            <h2 class="text-primary">Liste Membre etudiant</h2>
        </div>
    </div>

    <br /><br />

    @if (Model.Count() > 0)
    {
        <table class="table table-bordered table-striped" style="width:100%">
            <thead>
                <tr>
                    <th>
                        Nom
                    </th>
                    <th>
                        Email
                    </th>

                    <th>
                        Specialite
                    </th>
                    <th>
                        Niveau
                    </th>
                    <th>
                        Nom club
                    </th>
                    
                </tr>
            </thead>
            <tbody>
                @foreach (var obj in Model)

                {

                    
                    <tr>
                        <td width="20%">@obj.Nom</td>
                        <td width="33%">@obj.email</td>
                        <td width="20%">@obj.specialite</td>
                        <td width="10%">@obj.niveau</td>
                        <td width="45%">@obj.nameclub</td>
                    </tr>
                }
            </tbody>
        </table>
    }
    else
    {
        <p> No membre exists.</p>
    }

</div>